Output generated: <%= Time.now %>
<pre>
$ qlist
</pre>
<table class="table table-bordered">
  <tr>
    <th>Queue (partition)</th>
    <th>Total Cores</th>
    <th>Free Cores</th>
    <th>Jobs Running</th>
    <th>Jobs Pending</th>
    <th>Time Limit</th>
    <th>SU Charge</th>
  </tr>
  <% @processes.each do |app| %>
  <tr>
    <td><%= app.queue %></td>
    <td><%= app.total_cores %></td>
    <td><%= app.free_cores %></td>
    <td><%= app.jobs_running %></td>
    <td><%= app.jobs_pending %></td>
    <td><%= app.time_limit %></td>
    <td><%= app.su_charge %></td>
  </tr>
  <% end %>

</table>